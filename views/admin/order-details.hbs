      <div class="page-content form-page">
            <!-- Page Header-->
            <div class="bg-dash-dark-2 py-4">
              <div class="container-fluid">
                <h2 class="h5 mb-0">Order Details</h2>
              </div>
            </div>
            <!-- Breadcrumb-->
            <div class="container-fluid py-2">
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb mb-0 py-3 px-0">
                  <li class="breadcrumb-item"><a href="/admin">Home</a></li>
                  <li class="breadcrumb-item active" aria-current="page">Order Details</li>
                </ol>
              </nav>
            </div>
        <section class="tables py-0">
          <div class="container-fluid">
            <div class="row gy-4">
            
              <div class="">
                <div class="card mb-0">
                  <div class="card-header">
                    <h3 class="h4 mb-0 text-secondary">Order Details</h3>
                  </div>
                  <div class="card-body pt-0">
                    <div class="table-responsive">
                      <table class="table mb-0 table-striped table-hover ">
                        <thead>
                          <tr>
                            <th class="text-secondary">Sl No</th>
                            <th class="text-secondary">Product</th>
                            <th class="text-secondary">Product Name</th>
                            <th class="text-secondary">Category</th>
                            <th class="text-secondary">quantity</th>
                            <th class="text-secondary">Price</th>
                            <th class="text-secondary">Status</th>
                          </tr>
                        </thead>
                        <tbody>
                            {{#each orderDetail.products}}
                          <tr>
                            <th class="text-secondary" scope="row">{{math @index "+" 1}}</th>
                            <td class="text-secondary"><img style="width: 100px;height: 100px; " src="/product-images/{{this.products._id}}0.jpg" alt=""></td>
                            <td class="text-secondary">{{this.products.Name}}</td>
                            <td class="text-secondary">{{this.products.Category}}</td>
                            <td class="text-secondary">{{this.products.quantity}}</td>
                            <td class="text-secondary">{{this.products.Price}}</td>
                            <td class="text-secondary">{{../orderDetail.status}}</td>
                            {{!-- <td class="text-secondary"><button onclick="deleteproduct('{{this._id}}','{{this.imageqty}}')" class="btn btn-outline-danger">Delete</button></td>
                            <td class="text-secondary"><a class="btn btn-outline-info px-3" href="/admin/edit-product?id={{this._id}}">Edit</a></td> --}}
                          </tr>
                          {{/each}}
                         
                        </tbody>
                      </table>
                      <div class="d-flex justify-content-between pt-2">
              <p class="fw-bold mb-0 text-white">Order Details</p>
              <p class="fw-bold mb-0 text-white">Address Details</p>
            </div>


            <div class="d-flex justify-content-between pt-2">
              <p class="text-muted mb-0"><span class="fw-bold me-2">Invoice Number</span>: {{orderDetail.orderId}}</p>
              <p class="text-muted mb-0">{{orderDetail.deliveryDetails.name}}</p>
            </div>

            <div class="d-flex justify-content-between">
              <p class="text-muted mb-0"><span class="fw-bold me-2">Invoice Date</span>: {{orderDetail.date}}</p>
              <p class="text-muted mb-0">{{orderDetail.deliveryDetails.House_Name}}, {{orderDetail.deliveryDetails.street}}</p>
            </div>
            
            <div class="d-flex justify-content-between">
              <p class="text-muted mb-0"><span class="fw-bold me-2">Payment Method</span>: {{orderDetail.payment-method}}</p>
              <p class="text-muted mb-0">{{orderDetail.deliveryDetails.City}}, {{orderDetail.deliveryDetails.state}}</p>  
            </div>

             <div class="d-flex justify-content-between">
              <p class="text-muted mb-0"><span class="fw-bold me-2">Total</span>: {{orderDetail.totalAmount}}</p>
              <p class="text-muted mb-0">{{orderDetail.deliveryDetails.pincode}}, {{orderDetail.deliveryDetails.mobile}}</p>
            </div>
                    </div>
                  </div>
                </div>
              </div>
             
            </div>
          </div>
        </section>
        <!-- Page Footer-->
        <footer class="position-absolute bottom-0 bg-dash-dark-2 text-white text-center py-3 w-100 text-xs" id="footer">
