        <section class="tables py-0">
          <div class="container-fluid">
            <div class="row gy-4">
            
              <div class="">
                <div class="card mb-0">
                  <div class="card-header">
                    <h3 class="h4 mb-0 text-secondary">Product Management</h3>
                  </div>
                  <div class="card-body pt-0">
                    <div class="table-responsive">
                      <table class="table mb-0 table-striped table-hover ">
                        <thead>
                          <tr>
                            <th class="text-secondary">Sl No</th>
                            <th class="text-secondary">Product Name</th>
                            <th class="text-secondary">Category</th>
                            <th class="text-secondary">Stock</th>
                            <th class="text-secondary">Price</th>
                            <th class="text-secondary">Image</th>
                            <th class="text-secondary">Description</th>
                            <th class="text-secondary">Action</th>
                            <th class="text-secondary">Edit</th>
                          </tr>
                        </thead>
                        <tbody>
                            {{#each products}}
                          <tr>
                            <th class="text-secondary" scope="row">{{math @index "+" 1}}</th>
                            <td class="text-secondary">{{this.Name}}</td>
                            <td class="text-secondary">{{this.Category}}</td>
                            <td class="text-secondary">{{this.Stock}}</td>
                            <td class="text-secondary">{{this.Price}}</td>
                            <td class="text-secondary"><img style="width: 50px;height: 50px; " src="/product-images/{{this._id}}0.jpg" alt=""></td>
                            <td class="text-secondary">{{this.Description}}</td>
                            <td class="text-secondary"><button onclick="deleteproduct('{{this._id}}','{{this.imageqty}}')" class="btn btn-outline-danger">Delete</button></td>
                            <td class="text-secondary"><a class="btn btn-outline-info px-3" href="/admin/edit-product?id={{this._id}}">Edit</a></td>
                          </tr>
                          {{/each}}
                       
                        </tbody>
                      </table>
                    </div>
